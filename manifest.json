{
  "name": "NWMan: New Tab Extension",
  "title": "NWMan: New Tab Extension",
  "short_name": "ManChow",
  "version": "0.3",
  "manifest_version": 2,
  "description": "A simple Chrome new tab extension to start the semester.",
  "browser_action": {
    "default_icon": "./icon_36.png",
    "default_title": "NWCase: New Tab Extension"
  },
  "author": "Manoj Nuvvala",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  },
  "homepage_url": "http://denisecase.github.io/",
  "incognito": "spanning",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "aString"
  },
  "permissions": [
    "tabs",
    "geolocation",
    "<all_urls>",
    "https://www.bing.com/HPImageArchive.aspx?format=js&idx=0&n=1",
    "https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js",
    "./favicon.ico",
    "https://img.buzzfeed.com/buzzfeed-static/static/2017-04/19/15/asset/buzzfeed-prod-fastlane-01/sub-buzz-7887-1492630453-6.jpg",
    "https://img.buzzfeed.com/buzzfeed-static/static/2017-04/19/15/asset/buzzfeed-prod-fastlane-03/sub-buzz-6069-1492630255-3.jpg",
    "https://img.buzzfeed.com/buzzfeed-static/static/2017-04/19/15/asset/buzzfeed-prod-fastlane-03/sub-buzz-6150-1492630238-1.jpg"
  ],
  "web_accessible_resources": ["./favicon.ico"]
}
